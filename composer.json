{
    "name": "kimonocode/brikphp",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "kimonocode"
        }
    ],
    "autoload": {
        "psr-4": {
            "Brikphp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Brikphp\\Tests\\" : "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "http-interop/response-sender": "^1.0",
        "php-di/php-di": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "psr/http-server-middleware": "^1.0",
        "middlewares/whoops": "^2.0",
        "kimonocode/brikphp-filesystem": "main-dev"
    }
}
